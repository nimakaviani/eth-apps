pragma solidity ^0.4.2;

contract ContractD {
    string a;
    string b;
    string c;

    function ContractD(string _a, string _b, string _c) {
        a = _a;
        b = _b;
        c = _c;
    }

    function getA() constant returns (string) {
        return a;
    }

    function getB() constant returns (string) {
        return b;
    }

    function getC() constant returns (string) {
        return c;
    }
}
